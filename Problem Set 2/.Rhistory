summarize(avg_don_rate = mean(donors), sd_don_rate = sd(donors)) %>%
ggplot(aes(x = year, y = avg_don_rate, color = consent_law)) +
geom_line(size = 1) +
geom_ribbon(aes(ymin = avg_don_rate - sd_don_rate,
ymax = avg_don_rate + sd_don_rate))
